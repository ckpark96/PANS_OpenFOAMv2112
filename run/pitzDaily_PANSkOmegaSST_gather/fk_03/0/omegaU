FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omegaU;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 39.613333;
fk 0.3;

boundaryField
{
    inlet
    {
        type            inletOutlet;
        inletValue           $internalField / fk;
    }

    outlet
    {
        type            zeroGradient;
    }

    upperWall
    {
        type            omegaWallFunction;
        value           $internalField / fk;
    }

    lowerWall
    {
        type            omegaWallFunction;
        value           $internalField / fk;
    }

    frontAndBack
    {
        type            empty;
    }
}


// ************************************************************************* //
