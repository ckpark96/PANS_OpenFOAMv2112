FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1e-8;

boundaryField
{
    inlet
    {
        type            calculated;
        value           $internalField;
    }

    outlet
    {
        type            calculated;
        value           $internalField;
    }

    upperWall
    {
        type            nutkWallFunction; //nutUSpaldingWallFunction;  //fixedValue;
        value		$internalField;
    }

    lowerWall
    {
        type            nutkWallFunction; //nutUSpaldingWallFunction; //fixedValue;
        value		$internalField;
    }

    front
    {
        type            cyclic;
    }
    
    back
    {
        type            cyclic;
    }
}


// ************************************************************************* //
